{
    "extends": "eslint:recommended",
    "plugins": [
        "mocha"
    ],
    "env": {
        "mocha": true,
        "amd": true,
        "browser": true,
        "commonjs": true,
        "node": true,
        "es6": true
    },
    "rules": {
        "indent": [2, 4, { "SwitchCase": 1 }],
        "no-tabs": 0,
        "no-console": "off",
        "no-async-promise-executor": 0
    },
    "parserOptions": {
        "ecmaVersion": 8
    }
}
